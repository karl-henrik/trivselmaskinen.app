<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
					   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
					   x:Class="Trivselappen.Views.MainView">
  <StackLayout Spacing="30">
    <Image Source="Sogeti.png"></Image>
    <Label Text="Välkommen till Sogetis trivselapp!" Font="Medium" VerticalOptions="Center" HorizontalOptions="Center" />
    <Button Text="Enter token" Command="{Binding Token}" />
    <Button Text="Get question" Command="{Binding Question}" />
  </StackLayout>
</ContentPage>